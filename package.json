{
  "name": "manekiswap-contracts",
  "version": "1.0.0",
  "description": "Contracts for the maneki protocol",
  "scripts": {
    "server": "npx hardhat node",
    "build": "npm run clean && npm run compile",
    "clean": "npx hardhat clean",
    "test": "TS_NODE_TRANSPILE_ONLY=1 npx hardhat test",
    "compile": "npx hardhat compile",
    "ropsten:deploy": "hardhat run --network ropsten scripts/deploy_maneki.ts",
    "ropsten:verify": "hardhat run --network ropsten etherscan-verify",
    "rinkeby:deploy": "hardhat run --network rinkeby scripts/deploy_maneki.ts",
    "rinkeby:verify": "hardhat --network rinkeby etherscan-verify --solc-input",
    "kovan:deploy": "hardhat run --network rinkeby scripts/deploy_maneki.ts",
    "kovan:verify": "hardhat run --network rinkeby etherscan-verify ",
    "local:deploy": "hardhat run --network localhost scripts/deploy_maneki.ts",
    "deploy_swap": "TS_NODE_TRANSPILE_ONLY=1 hardhat run --network localhost scripts/deploy_swap.ts",
    "deploy_pair": "TS_NODE_TRANSPILE_ONLY=1 hardhat run --network localhost scripts/create_pair.ts"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "2.0.2",
    "@nomiclabs/hardhat-etherscan": "2.1.7",
    "@nomiclabs/hardhat-waffle": "2.0.1",
    "@nomiclabs/hardhat-web3": "2.0.0",
    "@openzeppelin/contracts": "3.4.1-solc-0.7",
    "@openzeppelin/contracts-upgradeable": "3.4.1-solc-0.7",
    "@openzeppelin/hardhat-upgrades": "1.12.0",
    "@openzeppelin/test-helpers": "0.5.15",
    "@typechain/ethers-v5": "8.0.2",
    "@typechain/hardhat": "3.0.0",
    "@types/mocha": "9.0.0",
    "@types/node": "15.12.4",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/parser": "5.3.1",
    "chai": "4.3.4",
    "dotenv": "10.0.0",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "ethereum-waffle": "3.4.0",
    "ethers": "5.5.1",
    "hardhat": "2.6.8",
    "hardhat-deploy": "0.9.5",
    "hardhat-deploy-ethers": "0.3.0-beta.11",
    "hardhat-preprocessor": "0.1.4",
    "hardhat-typechain": "0.3.5",
    "prettier": "2.4.1",
    "prettier-plugin-solidity": "1.0.0-beta.18",
    "solhint": "3.3.6",
    "ts-generator": "0.1.1",
    "ts-node": "10.4.0",
    "typechain": "6.0.2",
    "typescript": "4.4.4",
    "web3": "1.6.0"
  },
  "dependencies": {
    "mnemonic": "1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manekiswap/manekiswap-contracts.git"
  },
  "keywords": [],
  "author": "croccifixio-maneki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/manekiswap/manekiswap-contracts/issues"
  },
  "homepage": "https://github.com/manekiswap/manekiswap-contracts#readme"
}
